var split = function (time) {
  if(time){
    var d = getDate(time*1000)
    var batchTime = d.getFullYear() + '-' + (d.getMonth() + 1) + '-' + d.getDate() + ' ' + d.getHours() + ':' + d.getMinutes() + ':' + d.getSeconds();
    return batchTime
  }else{
    return
  }
}
module.exports = {
  split: split,
}