/* 搜索框 */
.inputsearch {
    width: 57%;
    padding-right: 10rpx;
    box-sizing: border-box;
    position: relative;
    padding-top: 8rpx;
}
.search__input {
    width: 395rpx;
    height: 80rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-family: UICTFontTextStyleBody;
    min-height: 30px;
    box-sizing: border-box;
    padding: 2% 6%;
    font-size: 28rpx;
    background: #F7F7F7;
    border-radius: 10rpx;
    margin: 13rpx 30rpx;
    color: #cccccc;
    border: 2rpx #cccccc solid;
}
.search__active {
  position: absolute;
  top: 33%;
  right: 5%;
}






/* 菜单栏 */
.clear{
clear: both;
