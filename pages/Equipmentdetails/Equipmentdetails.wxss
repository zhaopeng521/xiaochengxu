}

.tabTit{
height:90rpx;
line-height: 90rpx;
border-bottom: solid 1px #eee;
box-shadow:0px 15px 10px -15px #ccc;
top:0;
width: 750rpx;
z-index: 1;
background: #fff;
font-size:26rpx;
text-align: center;
}



.tabTit .active{
color:#3A80E2;
background: #fff;
}

.tabTit .active image{
transform: rotate(180deg);
