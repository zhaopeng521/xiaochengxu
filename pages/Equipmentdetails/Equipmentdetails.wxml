    <view class="serve-bottom">
        <view class="expert-list" bindsubmit="saveFormId" data-id="{{item.id}}" reportSubmit="{{true}}" wx:for="{{allArr}}" bindtap="xiangqing" data-item="{{item}}" wx:key="*this">
          <view class="expert-list-left">
            <image src="/tabbar/fengjitu.jpg"></image>
          </view>
          <view class="expert-list-right">
            <view class="expert-name">
              <view class="box1">
                <view style="font-size: 36rpx;font-weight: 600;color: #000;width: 85%;">{{item.name}}</view>          
                <view class="{{statusColor[item.status]}}">{{item.status}}</view>
              </view>
            </view>
            <view class="expert-num">
              <view>
                <text style="#666666" wx:if="{{item.type == null}}">产品系列：无</text>
                <text style="#666666" wx:else>产品系列：{{item.type}}</text>
              </view>
            </view>
            <view class="expert-num">
              <view>
                <text style="#666666" wx:if="{{item.device_num == null}}">产品编号：无</text>
                <text style="#666666" wx:else>产品编号：{{item.device_num}}</text>
              </view>
            </view>
            <view class="expert-num">
